@model IEnumerable<Web.Models.AssignUsersToRole>

@{
    ViewBag.Title = "Index";
}
<html>
<head>
    <link rel="stylesheet" href="~/Content/Site.css" />
</head>
<body class="listcss">
    <div class="Home">
        <h2><span style="color:darkseagreen">Index</span> </h2>
    </div>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Employee.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role.RoleName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Employee.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Role.RoleName)
                </td>

            </tr>
        }

    </table>
    </body>
</html>